# Clock signal
set_property PACKAGE_PIN W5 [get_ports clock_at_100mhz]							
	set_property IOSTANDARD LVCMOS33 [get_ports clock_at_100mhz]
	create_clock -add -name sys_clk_pin -period 10.00 -waveform {0 5} [get_ports clock_at_100mhz]
 
